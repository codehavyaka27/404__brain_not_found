    {
      "cell_type": "code",
      "source": [
        "import torch\n",
        "import torch.nn as nn\n",
        "import torch.optim as optim\n",
        "from torch.utils.data import DataLoader, random_split, Subset\n",
        "from torchvision import datasets, models, transforms\n",
        "import os\n",
        "import numpy as np\n",
        "import copy\n",
        "import time\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "# --- NEW IMPORTS for DP and Encryption ---\n",
        "from opacus import PrivacyEngine\n",
        "from cryptography.fernet import Fernet\n",
        "import pickle\n",
        "\n",
        "# --- 1. Define Helper Functions and Classes (Same as Phase 2) ---\n",
        "\n",
        "def get_model(num_classes):\n",
        "    \"\"\"Loads a pretrained ResNet18 model and replaces the final layer.\"\"\"\n",
        "    model = models.resnet18(weights='IMAGENET1K_V1')\n",
        "\n",
        "    # Freeze all layers\n",
        "    for param in model.parameters():\n",
        "        param.requires_grad = False\n",
        "\n",
        "    num_ftrs = model.fc.in_features\n",
        "    # Replace the final layer\n",
        "    model.fc = nn.Linear(num_ftrs, num_classes)\n",
        "    return model\n",
        "\n",
        "def create_non_iid_split(dataset, num_clients, alpha=0.5):\n",
        "    \"\"\"Splits a PyTorch training dataset into non-IID partitions... (Same as Phase 2)\"\"\"\n",
        "    # ... (This function is identical to Phase 2, so I'll trim it for brevity)\n",
        "    if isinstance(dataset, Subset):\n",
        "        targets = np.array([dataset.dataset.targets[i] for i in dataset.indices])\n",
        "        original_dataset = dataset.dataset\n",
        "    else:\n",
        "        targets = np.array(dataset.targets)\n",
        "        original_dataset = dataset\n",
        "    num_indices = len(dataset)\n",
        "    indices = list(range(num_indices))\n",
        "    num_classes = len(np.unique(targets))\n",
        "    label_distribution = np.random.dirichlet([alpha] * num_classes, num_clients)\n",
        "    class_indices = [np.where(targets == i)[0] for i in range(num_classes)]\n",
        "    client_indices = [[] for _ in range(num_clients)]\n",
        "    for c in range(num_classes):\n",
        "        class_idx = class_indices[c]\n",
        "        np.random.shuffle(class_idx)\n",
        "        proportions = np.cumsum(label_distribution[:, c])\n",
        "        proportions = (proportions / proportions[-1] * len(class_idx)).astype(int)\n",
        "        proportions = np.split(class_idx, proportions[:-1])\n",
        "        for i in range(num_clients):\n",
        "            if isinstance(dataset, Subset):\n",
        "                original_idx = [dataset.indices[j] for j in proportions[i]]\n",
        "                client_indices[i].extend(original_idx)\n",
        "            else:\n",
        "                client_indices[i].extend(proportions[i])\n",
        "    client_dataloaders = []\n",
        "    for i in range(num_clients):\n",
        "        client_dataset = Subset(original_dataset, client_indices[i])\n",
        "        # We need to re-wrap the dataloader for Opacus, so batch size is important\n",
        "        loader = DataLoader(client_dataset, batch_size=32, shuffle=True, num_workers=4)\n",
        "        client_dataloaders.append(loader)\n",
        "        print(f\"Client {i}: {len(client_indices[i])} samples\")\n",
        "    return client_dataloaders\n",
        "\n",
        "# --- 2. Define Client (Hospital) Class ---\n",
        "#    *** THIS CLASS IS HEAVILY MODIFIED ***\n",
        "class Client:\n",
        "    def __init__(self, client_id, dataloader, num_classes, device,\n",
        "                 noise_multiplier, max_grad_norm, delta):\n",
        "\n",
        "        self.client_id = client_id\n",
        "        self.device = device\n",
        "        self.delta = delta\n",
        "\n",
        "        self.model = get_model(num_classes).to(self.device)\n",
        "        self.criterion = nn.CrossEntropyLoss()\n",
        "        # We unfreeze the final layer for training\n",
        "        self.optimizer = optim.SGD(self.model.fc.parameters(), lr=0.001, momentum=0.9)\n",
        "\n",
        "        # --- Attach Opacus PrivacyEngine ---\n",
        "        self.privacy_engine = PrivacyEngine()\n",
        "\n",
        "        # Opacus requires the dataloader to be \"wrapped\"\n",
        "        # We must re-create the dataloader with the *exact same* dataset\n",
        "        # This is a quirk of Opacus: it needs the original dataloader\n",
        "        self.dataloader = DataLoader(dataloader.dataset, batch_size=32, shuffle=True)\n",
        "\n",
        "        self.model, self.optimizer, self.dataloader = self.privacy_engine.make_private(\n",
        "            module=self.model,\n",
        "            optimizer=self.optimizer,\n",
        "            data_loader=self.dataloader,\n",
        "            noise_multiplier=noise_multiplier,\n",
        "            max_grad_norm=max_grad_norm,\n",
        "        )\n",
        "\n",
        "        print(f\"Client {client_id}: Opacus PrivacyEngine attached.\")\n",
        "\n",
        "    def set_global_model(self, global_model_state_dict):\n",
        "      \"\"\"Loads the weights from the central server.\"\"\"\n",
        "      # Load into the *underlying* model, not the Opacus wrapper\n",
        "      self.model._module.load_state_dict(global_model_state_dict)\n",
        "\n",
        "    def train(self, local_epochs=1):\n",
        "        \"\"\"Trains the model on its local data with DP.\"\"\"\n",
        "        self.model.train()\n",
        "        for epoch in range(local_epochs):\n",
        "            for inputs, labels in self.dataloader:\n",
        "                inputs, labels = inputs.to(self.device), labels.to(self.device)\n",
        "\n",
        "                self.optimizer.zero_grad()\n",
        "                outputs = self.model(inputs)\n",
        "                loss = self.criterion(outputs, labels)\n",
        "                loss.backward()\n",
        "                self.optimizer.step()\n",
        "\n",
        "        # --- Get Privacy Cost ---\n",
        "        epsilon = self.privacy_engine.get_epsilon(delta=self.delta)\n",
        "        print(f\"Client {self.client_id} finished training. Epsilon: {epsilon:.4f}\")\n",
        "        return epsilon\n",
        "\n",
        "    def get_encrypted_weights(self, cipher_suite):\n",
        "        \"\"\"Returns its locally trained model weights as encrypted bytes.\"\"\"\n",
        "        # Get weights from the *underlying* model, not the Opacus wrapper\n",
        "        weights = self.model._module.state_dict()\n",
        "\n",
        "        # 1. Serialize: Convert weights (dict) to bytes\n",
        "        serialized_weights = pickle.dumps(weights)\n",
        "\n",
        "        # 2. Encrypt: Convert bytes to encrypted bytes\n",
        "        encrypted_weights = cipher_suite.encrypt(serialized_weights)\n",
        "\n",
        "        return encrypted_weights\n",
        "\n",
        "# --- 3. Define Server (Aggregator) Class ---\n",
        "#    *** THIS CLASS IS MODIFIED ***\n",
        "class Server:\n",
        "    def __init__(self, global_model, cipher_suite):\n",
        "        self.global_model = global_model\n",
        "        self.cipher_suite = cipher_suite # Store the key\n",
        "\n",
        "    def aggregate_weights(self, encrypted_weights_list):\n",
        "        \"\"\"Performs FedAvg on ENCRYPTED weights.\"\"\"\n",
        "\n",
        "        client_weights_list = []\n",
        "\n",
        "        # --- Decrypt weights first ---\n",
        "        for enc_weights in encrypted_weights_list:\n",
        "            # 1. Decrypt: Convert encrypted bytes back to bytes\n",
        "            decrypted_bytes = self.cipher_suite.decrypt(enc_weights)\n",
        "\n",
        "            # 2. Deserialize: Convert bytes back to weights (dict)\n",
        "            weights = pickle.loads(decrypted_bytes)\n",
        "            client_weights_list.append(weights)\n",
        "\n",
        "        # --- Aggregation logic (same as Phase 2) ---\n",
        "        global_state_dict = self.global_model.state_dict()\n",
        "        for key in global_state_dict:\n",
        "            global_state_dict[key] = torch.zeros_like(global_state_dict[key])\n",
        "\n",
        "        for client_weights in client_weights_list:\n",
        "            for key in global_state_dict:\n",
        "                global_state_dict[key] += client_weights[key]\n",
        "\n",
        "        num_clients = len(client_weights_list)\n",
        "        for key in global_state_dict:\n",
        "            if global_state_dict[key].dtype == torch.float32:\n",
        "                global_state_dict[key] /= num_clients\n",
        "            else:\n",
        "                global_state_dict[key] //= num_clients\n",
        "\n",
        "        self.global_model.load_state_dict(global_state_dict)\n",
        "        print(\"Server finished aggregation on decrypted weights.\")\n",
        "\n",
        "    def get_global_model_state(self):\n",
        "        return self.global_model.state_dict()\n",
        "\n",
        "# --- 4. Evaluation Function (Same as Phase 2) ---\n",
        "def evaluate_model(model, dataloader, device):\n",
        "    \"\"\"Evaluates the global model on the central test set.\"\"\"\n",
        "    model.eval()\n",
        "    running_corrects = 0\n",
        "    total = 0\n",
        "\n",
        "    with torch.no_grad():\n",
        "        for inputs, labels in dataloader:\n",
        "            inputs, labels = inputs.to(device), labels.to(device)\n",
        "            outputs = model(inputs)\n",
        "            _, preds = torch.max(outputs, 1)\n",
        "\n",
        "            total += labels.size(0)\n",
        "            running_corrects += torch.sum(preds == labels.data)\n",
        "\n",
        "    accuracy = running_corrects.double() / total\n",
        "    return accuracy.item()\n",
        "\n",
        "# --- 5. Main Execution ---\n",
        "#    *** THIS BLOCK IS MODIFIED ***\n",
        "if __name__ == '__main__':\n",
        "\n",
        "    # --- Configuration ---\n",
        "    NUM_CLIENTS = 3\n",
        "    NUM_ROUNDS = 20\n",
        "    LOCAL_EPOCHS = 3\n",
        "    DIRICHLET_ALPHA = 0.5\n",
        "    DATA_DIR = 'dataset_root'\n",
        "\n",
        "    # --- NEW: Privacy and Security Config ---\n",
        "    NOISE_MULTIPLIER = 0.5  # Controls noise level. Try 0.5, 1.0, 1.5\n",
        "    MAX_GRAD_NORM = 1.0     # Clipping bound\n",
        "    DELTA = 1e-5            # Target delta for DP\n",
        "\n",
        "    print(f\"--- Phase 3: FL + DP + Encryption ---\")\n",
        "    print(f\"Config: {NUM_CLIENTS} Clients, {NUM_ROUNDS} Rounds, {LOCAL_EPOCHS} Local Epochs\")\n",
        "    print(f\"DP Config: Noise={NOISE_MULTIPLIER}, MaxGradNorm={MAX_GRAD_NORM}, Delta={DELTA}\")\n",
        "\n",
        "    # --- NEW: Setup Encryption ---\n",
        "    key = Fernet.generate_key()\n",
        "    cipher_suite = Fernet(key)\n",
        "    print(f\"Generated encryption key (do not share): {key.decode()}\")\n",
        "    # Clear log file for new run\n",
        "    if os.path.exists(\"encrypted_transmission.log\"):\n",
        "        os.remove(\"encrypted_transmission.log\")\n",
        "\n",
        "    # --- Setup Device ---\n",
        "    device = torch.device(\"cuda:0\" if torch.cuda.is_available() else \"cpu\")\n",
        "    print(f\"Using device: {device}\")\n",
        "\n",
        "    # --- Setup Data (Same as Phase 2) ---\n",
        "    data_transforms = {\n",
        "        'train': transforms.Compose([\n",
        "            transforms.Resize(256), transforms.CenterCrop(224),\n",
        "            transforms.RandomHorizontalFlip(), transforms.ToTensor(),\n",
        "            transforms.Normalize([0.485, 0.456, 0.406], [0.229, 0.224, 0.225])\n",
        "        ]),\n",
        "        'val': transforms.Compose([\n",
        "            transforms.Resize(256), transforms.CenterCrop(224),\n",
        "            transforms.ToTensor(),\n",
        "            transforms.Normalize([0.485, 0.456, 0.406], [0.229, 0.224, 0.225])\n",
        "        ]),\n",
        "    }\n",
        "\n",
        "    full_dataset = datasets.ImageFolder(DATA_DIR, data_transforms['train'])\n",
        "    class_names = full_dataset.classes\n",
        "    num_classes = len(class_names)\n",
        "\n",
        "    torch.manual_seed(42)\n",
        "    test_split = 0.2\n",
        "    test_size = int(len(full_dataset) * test_split)\n",
        "    train_size = len(full_dataset) - test_size\n",
        "    train_dataset, test_dataset = random_split(full_dataset, [train_size, test_size])\n",
        "\n",
        "    test_dataset.dataset = copy.deepcopy(test_dataset.dataset)\n",
        "    test_dataset.dataset.transform = data_transforms['val']\n",
        "    test_loader = DataLoader(test_dataset, batch_size=32, shuffle=False, num_workers=4)\n",
        "\n",
        "    # --- Create Non-IID Client DataLoaders ---\n",
        "    print(\"\\nCreating non-IID data split for clients...\")\n",
        "    client_dataloaders = create_non_iid_split(train_dataset, NUM_CLIENTS, alpha=DIRICHLET_ALPHA)\n",
        "    for loader in client_dataloaders:\n",
        "        loader.dataset.dataset.transform = data_transforms['train']\n",
        "\n",
        "    # --- Initialize Server, Clients, and Global Model ---\n",
        "    global_model = get_model(num_classes).to(device)\n",
        "    server = Server(global_model, cipher_suite) # Pass cipher suite to server\n",
        "\n",
        "    # Pass privacy params to clients\n",
        "    clients = [Client(i, client_dataloaders[i], num_classes, device,\n",
        "                      NOISE_MULTIPLIER, MAX_GRAD_NORM, DELTA)\n",
        "               for i in range(NUM_CLIENTS)]\n",
        "\n",
        "    # --- Start FL Training Loop ---\n",
        "    print(\"\\n--- Starting DP-FL Training ---\")\n",
        "\n",
        "    round_numbers = []\n",
        "    global_accuracies = []\n",
        "    global_epsilons = [] # Track privacy cost\n",
        "    start_time = time.time()\n",
        "\n",
        "    for round in range(NUM_ROUNDS):\n",
        "        round_start_time = time.time()\n",
        "        print(f\"\\n--- Round {round+1}/{NUM_ROUNDS} ---\")\n",
        "\n",
        "        encrypted_weights_list = []\n",
        "        total_epsilon = 0.0\n",
        "\n",
        "        global_model_state = server.get_global_model_state()\n",
        "\n",
        "        for client in clients:\n",
        "            client.set_global_model(global_model_state)\n",
        "\n",
        "            # 2. Client trains locally (and returns epsilon)\n",
        "            epsilon = client.train(local_epochs=LOCAL_EPOCHS)\n",
        "            total_epsilon += epsilon\n",
        "\n",
        "            # 3. Client shares ENCRYPTED weights\n",
        "            enc_weights = client.get_encrypted_weights(cipher_suite)\n",
        "            encrypted_weights_list.append(enc_weights)\n",
        "\n",
        "            # --- Log encrypted transfer ---\n",
        "            with open(\"encrypted_transmission.log\", \"ab\") as f:\n",
        "                f.write(f\"--- Round {round+1}, Client {client.client_id} ---\\n\".encode('utf-8'))\n",
        "                f.write(enc_weights + b\"\\n\\n\")\n",
        "\n",
        "        # 4. Server aggregates the encrypted weights\n",
        "        server.aggregate_weights(encrypted_weights_list)\n",
        "\n",
        "        # 5. Evaluate the new global model\n",
        "        accuracy = evaluate_model(server.global_model, test_loader, device)\n",
        "        avg_epsilon = total_epsilon / NUM_CLIENTS\n",
        "\n",
        "        # Logging\n",
        "        round_numbers.append(round + 1)\n",
        "        global_accuracies.append(accuracy)\n",
        "        global_epsilons.append(avg_epsilon)\n",
        "\n",
        "        round_time = time.time() - round_start_time\n",
        "        print(f\"Round {round+1} Global Model Accuracy: {accuracy:.4f}\")\n",
        "        print(f\"Round {round+1} Epsilon (avg per client): {avg_epsilon:.4f}\")\n",
        "        print(f\"Round {round+1} took {round_time // 60:.0f}m {round_time % 60:.0f}s\")\n",
        "\n",
        "    total_time = time.time() - start_time\n",
        "    print(f\"\\n--- DP-FL Training Complete in {total_time // 60:.0f}m {total_time % 60:.0f}s ---\")\n",
        "\n",
        "    # --- Plotting the Results ---\n",
        "    fig, (ax1, ax2) = plt.subplots(2, 1, figsize=(10, 10))\n",
        "\n",
        "    # Plot 1: Accuracy\n",
        "    ax1.plot(round_numbers, global_accuracies, marker='o', color='b')\n",
        "    ax1.set_title('Global Model Accuracy vs. Communication Round (FL + DP)')\n",
        "    ax1.set_xlabel('Communication Round')\n",
        "    ax1.set_ylabel('Global Model Accuracy')\n",
        "    ax1.grid(True)\n",
        "\n",
        "    # Plot 2: Epsilon\n",
        "    ax2.plot(round_numbers, global_epsilons, marker='o', color='r')\n",
        "    ax2.set_title('Privacy Cost ($\\epsilon$) vs. Communication Round')\n",
        "    ax2.set_xlabel('Communication Round')\n",
        "    ax2.set_ylabel('Epsilon ($\\epsilon$)')\n",
        "    ax2.grid(True)\n",
        "\n",
        "    plt.tight_layout()\n",
        "    plt.savefig('fl_dp_encrypted_results.png')\n",
        "    plt.show()\n",
        "    print(\"Saved results plot as 'fl_dp_encrypted_results.png'\")\n",
        "\n",
        "    # Save the final DP FL model\n",
        "    torch.save(server.global_model.state_dict(), 'fl_dp_encrypted_model.pth')\n",
        "    print(\"Saved final FL+DP model as 'fl_dp_encrypted_model.pth'\")\n",
        "    print(\"Check 'encrypted_transmission.log' for simulated secure transfer logs.\")"
      ],
